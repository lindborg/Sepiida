package sepiida.weapons {	import sepiida.weapons.Weapon;		public class Bomb extends Weapon {		public function Bomb() {			speedRight = 20;			frictionRight = 1;			speedDown = Math.floor(Math.random()*5);			frictionDown = .5;			speedUp = Math.floor(Math.random()*5);			frictionUp = .5;			removeIdle = false;		}		override protected function speed():void {			super.speed();			rotation += 5;		}	}}